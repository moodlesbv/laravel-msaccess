{
    "name": "moodlesbv/laravel-msaccess",
    "description": "This package helps you to manage Microsoft Access DB by ODBC Connection with the integrated Laravel ORM.",
    "type": "library",
    "keywords": [
        "laravel",
        "orm",
        "microsoft-access"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zoilo Mora",
            "email": "zoilo.mora@hotmail.com",
            "homepage": "http://zoilomora.net",
            "role": "Developer"
        },
        {
            "name": "Martijn de Ridder",
            "email": "martijn@moodles.nl",
            "homepage": "https://www.moodles.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.6",
        "illuminate/database": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.4"
    },
    "autoload": {
        "psr-4": {
            "moodlesbv\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "moodlesbv\\AccessServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/moodlesbv/laravel-msaccess/issues"
    }
}
