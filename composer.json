{
    "name": "MoodlesBV/laravel-msaccess",
    "description": "This package helps you to manage Microsoft Access DB by ODBC Connection with the integrated Laravel ORM.",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "laravel",
        "orm",
        "microsoft-access"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zoilo Mora",
            "email": "zoilo.mora@hotmail.com",
            "homepage": "http://zoilomora.net",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.6",
        "illuminate/database": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.4"
    },
    "autoload": {
        "psr-4": {
            "MoodlesBV\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoodlesBV\\AccessServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/MoodlesBV/laravel-msaccess/issues"
    }
}
